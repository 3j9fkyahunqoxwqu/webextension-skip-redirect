{
    "applications": {
        "gecko": {
            "id": "skipredirect@sblask",
            "strict_min_version": "46.0"
        }
    },
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "base64.js",
            "url.js",
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": false,
        "default_icon": "icon.svg",
        "default_popup": "options/options.html",
        "default_title": "Skip Redirect"
    },
    "description": "Skip intermediary pages that some pages use before redirecting to a final page.",
    "homepage_url": "https://addons.mozilla.org/firefox/addon/skip-redirect/",
    "icons": {
        "128": "icon-128x128.png",
        "16": "icon-16x16.png",
        "32": "icon-32x32.png",
        "48": "icon-48x48.png",
        "64": "icon-64x64.png"
    },
    "manifest_version": 2,
    "name": "Skip Redirect",
    "options_ui": {
        "page": "options/options.html"
    },
    "permissions": [
        "<all_urls>",
        "notifications",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ],
    "version": "2.1.2"
}
